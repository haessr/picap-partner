<table>
  <thead>
    <tr>
      <th>Picap</th>
      <th>Reference</th>
      <th>Log</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.picap_id %></td>
        <td><%= booking.log %></td>
        <td><%= booking.reference %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>